{
  "name": "algo-mimc-hasher",
  "version": "1.0.6",
  "description": "Algorand App that implements a MIMC Hasher",
  "scripts": {
    "test": "jest",
    "start": "ts-node ./src/index.ts",
    "contract-build": "sh ./src/PuyaContracts/build.sh",
    "build": "rollup --resolveJsonModule --config rollup.config.mjs"
  },
  "author": "Winton Nathan-Roberts",
  "dependencies": {
    "@algorandfoundation/algokit-utils": "^6.2.1",
    "algosdk": "^2.9.0",
    "dotenv": "^16.4.5",
    "js-sha512": "^0.9.0",
    "mimc-hasher": "^1.0.9"
  },
  "main": "./dist/bundle.cjs",
  "module": "./dist/bundle.mjs",
  "license": "GPL-3.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/bundle.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/bundle.cjs"
      }
    }
  },
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Helium-Labs/Algo-MiMC-Hasher"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/browserify": "^12.0.37",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.4",
    "algokit-testkit": "^1.1.1",
    "jest": "^29.7.0",
    "rollup": "^4.9.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.4",
    "typescript": "^5.3.3"
  }
}
